@model List<CookBook.Models.RecipeType>

@*za svaki tip recepta koji postoji u modelu pravi se link u navbar-u i poziva se akcija GetRecipesByType*@
@foreach (var item in Model)
{
    <li class="nav-item active">@Html.ActionLink(item.Name, "GetRecipesByType", "Recipes", routeValues: new { @id = item.Id }, htmlAttributes: new { @class = "nav-link myClassForActionLink", @style = "color:white" })</li>
}